<?xml version="1.0"?>
<opencv_storage>

<capturer_impl_name>fld_capturer</capturer_impl_name>

<capturer_type>frontal</capturer_type>

<detector_type>cc4_with_angles</detector_type>


<min_size>30</min_size>

<max_size>10000</max_size>

<min_neighbors>3</min_neighbors>

<max_processed_width>640</max_processed_width>
<max_processed_height>480</max_processed_height>



<scale_factor>1.15</scale_factor>

<one_face>false</one_face>

<lbf_threshold>0.995</lbf_threshold>
<!-- <lbf_threshold>-0.275</lbf_threshold> -->

<lbf_original_size>25</lbf_original_size>

<lbf_detector_file>share/facedetectors/config_lbf/lbf_detector.data</lbf_detector_file>



<fitter_type>doublelbf</fitter_type>

<lbf_fitter1_file>share/lbffacefitter/lbf_fitter.data</lbf_fitter1_file>

<lbf_fitter2_file>share/lbffacefitter/lbf_fitter_29.data</lbf_fitter2_file>

<iris_enabled>0</iris_enabled>
<iris_config_filepath>conf/facerec/iris_fitter.xml</iris_config_filepath>



<downscale_rawsamples_to_preferred_size>1</downscale_rawsamples_to_preferred_size>

</opencv_storage>
