<?xml version="1.0"?>
<opencv_storage>

<capturer_impl_name>fld_capturer</capturer_impl_name>

<capturer_type>frontal</capturer_type>

<detector_type>cc4_with_angles</detector_type>


<min_size>45</min_size>
<max_size>10000</max_size>

<min_neighbors>1</min_neighbors>

<max_processed_width>320</max_processed_width>
<max_processed_height>240</max_processed_height>


<scale_factor>1.15</scale_factor>
<one_face>true</one_face>
<lbf_threshold>0.53</lbf_threshold>
<lbf_original_size>30</lbf_original_size>
<lbf_detector_file>share/facedetectors/config_lbf_noise/lbf_detector.data</lbf_detector_file>


<fitter_type>mesh</fitter_type>
<mesh_file>share/mesh/face_mesh.enc</mesh_file>

<iris_enabled>0</iris_enabled>
<iris_config_filepath>conf/facerec/iris_fitter.xml</iris_config_filepath>


<downscale_rawsamples_to_preferred_size>1</downscale_rawsamples_to_preferred_size>

</opencv_storage>
